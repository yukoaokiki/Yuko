<div class="tweets-container">
  <section id="first-block">
   <div class="title-container">
  
     <div class="title">
  
      

        <h1>新宿</h1>
  
     </div>
    </div>
  </section>
  
  
  <% @tweets.each do |t| %>
      <% if t.category == "tokyo" %>  
      <p>内容:<%= t.name %></p>
      <%= image_tag t.image_url, size: "250x200" if t.image? %>
  
      <% end %>
  <% end %>